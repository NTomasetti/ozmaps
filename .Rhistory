x$path_link_vertex$vertex_ <- match(x$path_link_vertex$vertex_,
x$vertex$vertex_)
x$path_link_vertex$vertex_ <- NULL
x$vertex$vertex_ <- NULL
x$path$path_ <- NULL
x
}
